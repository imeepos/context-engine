{
  "name": "@sker/prompt-renderer",
  "version": "0.1.0",
  "description": "React-based prompt renderer for AI agents",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@sker/compiler": "workspace:*",
    "@sker/core": "workspace:*",
    "react": "^18.3.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@sker/eslint-config": "workspace:*",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  }
}
