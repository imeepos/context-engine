{
  "name": "@sker/plugin",
  "version": "0.1.0",
  "description": "Unified plugin runtime/system package for Sker",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/index.cjs",
      "default": "./src/index.ts"
    },
    "./runtime": {
      "types": "./src/runtime/index.ts",
      "import": "./src/runtime/index.ts",
      "require": "./dist/runtime/index.cjs",
      "default": "./src/runtime/index.ts"
    },
    "./system": {
      "types": "./src/system/index.ts",
      "import": "./src/system/index.ts",
      "require": "./dist/system/index.cjs",
      "default": "./src/system/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts",
      "require": "./dist/types/index.cjs",
      "default": "./src/types/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@sker/core": "workspace:*",
    "@sker/prompt-renderer": "workspace:*",
    "react": "^18.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@sker/eslint-config": "workspace:*",
    "@sker/typescript-config": "workspace:*",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "react-dom": "^18.3.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  }
}

