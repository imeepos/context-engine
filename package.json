{
  "name": "claude-templates",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build": "pnpm -r --workspace-concurrency=1 build",
    "dev": "pnpm -r --workspace-concurrency=1 dev",
    "lint": "pnpm -r --workspace-concurrency=1 lint",
    "test": "pnpm -r --workspace-concurrency=1 test",
    "check-types": "pnpm -r --workspace-concurrency=1 check-types",
    "clean": "pnpm -r clean && rm -rf node_modules/.cache",
    "claude": "claude --dangerously-skip-permissions",
    "codex": "codex --dangerously-bypass-approvals-and-sandbox"
  },
  "packageManager": "pnpm@10.28.2"
}